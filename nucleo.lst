      33.  00:7EAC  78                  		ld	a,b				; a:=b (new ayFX stream index)
      34.  00:7EAD  BE                  		cp	[hl]				; If new index is not in the bank...
      35.  00:7EAE  3E 02               		ld	a,2				; a:=2 (error 2: Sample not in the bank)
      36.  00:7EB0  D2 12 7F            		jp	nc,.INIT_END			; ...we can't init it
      37.  00:7EB3                      .CHECK_PRI:	; --- Places the data on the correct channel                  ---
      38.  00:7EB3                      		; ---   Remember: 0 = highest priority, 15 = lowest priority  ---
      39.  00:7EB3                      		; --- Checks channel 1 ---
      40.  00:7EB3  3A 22 D4            		ld	a,[ayFX_C1]			; Priority of ayFX Stream being played on channel 1
      41.  00:7EB6  FE FF               		cp	255				; If there's no Stream being played...
      42.  00:7EB8  28 39               		jr	z,.TOC1			; ...place the new Stream on channel 1
      43.  00:7EBA  5F                  		ld	e,a				; Minimum priority of Streams being played
      44.  00:7EBB  16 01               		ld	d,1				; On channel 1
      45.  00:7EBD                      		; --- Checks channel 2 ---
      46.  00:7EBD  3A 25 D4            		ld	a,[ayFX_C2]			; Priority of ayFX Stream being played on channel 2
      47.  00:7EC0  FE FF               		cp	255				; If there's no Stream being played...
      48.  00:7EC2  28 29               		jr	z,.TOC2				; ...place the new Stream on channel 2
      49.  00:7EC4  BB                  		cp	e				; Compare priority with minimum one
      50.  00:7EC5  30 03               		jr	nc,.CHECKC3			; If P(C2) > min, go to .CHECKC3
      51.  00:7EC7  5F                  		ld	e,a				; Minimum priority of Streams being played
      52.  00:7EC8  16 02               		ld	d,2				; On channel 2
      53.  00:7ECA                      .CHECKC3:	; --- Checks channel 3 ---
      54.  00:7ECA  3A 28 D4            		ld	a,[ayFX_C3]			; Priority of ayFX Stream being played on channel 3
      55.  00:7ECD  FE FF               		cp	255				; If there's no stream being played
      56.  00:7ECF  28 16               		jr	z,.TOC3			; ...place the new Stream on channel 3
      57.  00:7ED1  BB                  		cp	e				; Compare priority with minimum one
      58.  00:7ED2  30 03               		jr	nc,.CHECKMIN			; If P(C3) > min, go to .CHECKMIN
      59.  00:7ED4  5F                  		ld	e,a				; Minimum priority of Streams being played
      60.  00:7ED5  16 03               		ld	d,3				; On channel 3
      61.  00:7ED7                      .CHECKMIN:	; --- Checks if minimum priority is lower than priority of the new Stream ---
      62.  00:7ED7  79                  		ld	a,c				; Priority of the new Stream
      63.  00:7ED8  BB                  		cp	e				; Minimum priority of Streams being played
      64.  00:7ED9  3E 01               		ld	a,1				; a:=1 (error 1: A sample with higher priority is being played)
      65.  00:7EDB  D2 12 7F            		jp	nc,.INIT_END			; ...we can't play the new ayFX Stream
      66.  00:7EDE                      		; --- The new stream substitues the minimum priority Stream being played ---
      67.  00:7EDE  7A                  		ld	a,d				; Channel of minimum priority
      68.  00:7EDF  FE 01               		cp	1				; If it's 1...
      69.  00:7EE1  28 10               		jr	z,.TOC1			; ...send Stream to channel 1
      70.  00:7EE3  FE 02               		cp	2				; If it's 2...
      71.  00:7EE5  28 06               		jr	z,.TOC2			; ...send Stream to channel 2
      72.  00:7EE7                      .TOC3:		; --- Stream will be played on channel 3
      73.  00:7EE7  DD 21 28 D4         		ld	IX,ayFX_C3			; Pointer to the chanel data
      74.  00:7EEB  18 0A               		jr	.SETDATA			; Go to .SETDATA
      75.  00:7EED                      .TOC2:		; --- Stream will be played on channel 2
      76.  00:7EED  DD 21 25 D4         		ld	IX,ayFX_C2			; Pointer to the channel data
      77.  00:7EF1  18 04               		jr	.SETDATA			; Go to .SETDATA
      78.  00:7EF3                      .TOC1:		; --- Stream will be played on channel 1
      79.  00:7EF3  DD 21 22 D4         		ld	IX,ayFX_C1			; Pointer to the chanel data
      80.  00:7EF7                      .SETDATA:	; --- Set data on the correct channel ---
      81.  00:7EF7  79                  		ld	a,c				; a:=New priority
      82.  00:7EF8  E6 0F               		and	$0F				; We mask the priority
      83.  00:7EFA  DD 77 00            		ld	[IX],a				; new ayFX stream priority saved in RAM
      84.  00:7EFD                      		; --- Calculate the pointer to the new ayFX stream ---
      85.  00:7EFD  ED 5B 20 D4         		ld	de,[ayFX_BANK]			; de:=Current ayFX bank
      86.  00:7F01  13                  		inc	de				; de points to the increments table of the bank
      87.  00:7F02  68                  		ld	l,b				; l:=b (new ayFX stream index)
      88.  00:7F03  26 00               		ld	h,0				; hl:=b (new ayFX stream index)
      89.  00:7F05  29                  		add	hl,hl				; hl:=hl*2
